{
    "name": "mxgma/swiss-bank-payment",
    "version": "2.0.2",
    "type": "library",
    "license": "MIT",
    "description": "PHP library to generate Swiss pain.001 messages (complies with ISO-20022)",
    "keywords": ["banking", "iso-20022", "six", "credit-transfer", "switzerland", "sepa", "xml"],
    "homepage": "https://github.com/mxgmacom/swiss-bank-payment",
    "authors": [
        {
            "name": "mxgmacom",
            "homepage": "https://github.com/mxgmacom",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --verbose tests"
    },
    "autoload": {
        "psr-0": {
            "Mxgma\\SwissBankPayment": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Mxgma\\SwissBankPayment\\Tests": "tests/"
        }
    }
}
